<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="700.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="yyg.rere.waiting.WaitController">
   <center>
      <ScrollPane prefHeight="632.0" prefWidth="752.0" BorderPane.alignment="CENTER">
         <content>
            <FlowPane fx:id="roomFlowPane" hgap="5.0" prefHeight="630.0" prefWidth="750.0" vgap="5.0">
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding></FlowPane>
         </content>
      </ScrollPane>
   </center>
   <right>
      <TableView fx:id="userTable" BorderPane.alignment="CENTER">
         <columns>
            <TableColumn prefWidth="100.0" text="순번" />
            <TableColumn prefWidth="100.0" text="닉네임" />
         </columns></TableView>
   </right>
   <bottom>
      <VBox alignment="CENTER_LEFT" spacing="5.0" BorderPane.alignment="CENTER">
         <children>
            <Separator prefWidth="200.0" />
            <HBox spacing="10.0">
               <children>
                  <GridPane alignment="CENTER">
                    <columnConstraints>
                      <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                    </columnConstraints>
                    <rowConstraints>
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <children>
                        <Label text="방제" textAlignment="CENTER" />
                        <TextField fx:id="roomName" prefHeight="50.0" prefWidth="400.0" GridPane.columnIndex="1" />
                     </children>
                  </GridPane>
                  <Button fx:id="btnCreateRoom" mnemonicParsing="false" prefHeight="50.0" prefWidth="500.0" text="방만들기" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </VBox>
   </bottom>
</BorderPane>
